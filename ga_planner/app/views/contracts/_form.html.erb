<%= form_for [@cohort, @contract] do |f| %>
  <div class= "field">
    <%= f.label :duration %><br>
    <%= f.text_field :duration%>
  </div>
  <div class= "field">
    <%= f.label :position %><br>
    <%= f.text_area :position%>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>

