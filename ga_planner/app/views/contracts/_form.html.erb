<%= form_for @contract do |f| %>
<p>
  <%= f.label :duration %><br>
  <%= f.text_field :duration%>
</p>
<p>
  <%= f.label :position %><br>
  <%= f.text_area :position%>
</p>
<p>
  <%= f.label :user_id %><br>
  <%= f.collection_select :user_id, User.all, :id, :name%>
</p>
<p>
  <%= f.label :cohort_id %><br>
  <%= f.collection_select :cohort_id, Cohort.all, :id, :name%>
</p>
<p>
  <%= f.submit %>
</p>
<% end %>

