<%= form_for [@cohort, @enrollment] do |f| %>

<p>
  <%= f.label :user_id %><br>
  <%= f.collection_select :user_id, User.all, :id, :name%>
</p>
<p>
  <%= f.label :start_date %><br>
  <%= f.text_field :start_date%>
</p>
<p>
  <%= f.label :pre_work %><br>
  <%= f.text_area :pre_work%>
</p>

<p>
  <%= f.submit %>
</p>
<% end %>

