<p>
 Location: <%= @classroom.location %><br>
</p>
<p>
  Room name: <%= @classroom.name %><br>
</p>
All bookings: <% @classroom.bookings.each do |booking| %>
<br>
Start date: <%= booking.start_date %>
<br>
End date: <%= booking.end_date %>
<%= booking.cohort.try :name %>
<% end %>

<%= link_to 'Back', classrooms_path, class: 'button'  %>

<br>

<h3>Listing Classrooms</h3>

<table>
  <tr>
    <th>Location</th>
    <th>Name</th>
  </tr>

<h3>Classrooms</h3>
<% @classroom.each do |room| %>
  <tr>
    <td><%= room.location %></td>
    <td><%= room.name %></td>
    <td><%= link_to 'book', new_classroom_booking_path(room) %></td>
  </tr>
<% end %>
</table>

