<%= form_for [@classroom, @booking] do |f| %>
<p>
  <%= f.label :start_date %><br>
  <%= f.date_select :start_date%>
</p>
<p>
  <%= f.label :end_date %><br>
 <%= f.date_select :end_date%>
</p>
<p>
  <%= f.label :cohort_id %><br>
  <%= f.collection_select :cohort_id, Cohort.all, :id, :name%>
</p>
  <%= f.submit %>
</p>
<% end %>

