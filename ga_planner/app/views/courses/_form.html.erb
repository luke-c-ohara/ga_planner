<%= form_for @course do |f| %>
    <p>
      <%= f.label :name %><br>
      <%= f.text_field :name %>
    </p>
    <p>
      <%= f.label :duration %><br>
      <%= f.text_area :duration%>
    </p>
      <%= f.label :course_type %><br>
      <%= f.text_area :course_type%>
   
    <p>
      <%= f.submit %>
    </p>
  <% end %>